{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base/legacy" 
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-var": "error", 
        "no-console": "off",
        "no-unused-vars": "off",
        "func-names": "off",
        "no-await-in-loop": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-restricted-syntax": "off"
    }
}
